{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "importHelpers": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "@services": [
        "./src/app/services/index"
      ],
      "@services/*": [
        "./src/app/services/*"
      ],
      "@enums/*": [
        "./src/app/enums/*"
      ],
      "@models": [
        "./src/app/models/index"
      ],
      "@models/*": [
        "./src/app/models/*"
      ],
      "@components/*": [
        "src/app/components/*"
      ],
      "@guards/*": [
        "src/app/guards/*"
      ],
      "@environment": ["./src/environments/environment.ts"]
    }
  }
}
